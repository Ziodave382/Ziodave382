import turtle



ninja = turtle.Turtle()
screen = turtle.Screen()
screen.bgcolor("black")
ninja.color("black")

ninja.speed(1999)
ninja.pensize(19)
ninja.hideturtle()
ninja.left(90)
ninja.forward(200)
ninja.left(-90)
ninja.color("green")
ninja.forward(300)
ninja.left(-90)
ninja.forward(40)
ninja.left(90)
ninja.forward(40)
ninja.left(-90)
ninja.forward(40)
